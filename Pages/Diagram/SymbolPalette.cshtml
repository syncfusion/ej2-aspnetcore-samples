@page
@model EJ2CoreSampleBrowser.Pages.Diagram.SymbolPaletteModel
@using Syncfusion.EJ2.Diagrams;
@{
    var getNodeDefaults = "getNodeDefaults";
    var getSymbolDefaults = "getSymbolDefaults";
    var getSymbolInfo = "getSymbolInfo";
    var valueChanged = "valueChanged";
    var expandValue = "Multiple";
}
@section ControlsSection{
    <div class="control-section">
        <div style="width: 100%;" class="diagram-symbolpalette">
            <div class="palette-column" style="min-width: 20%;">
                <div class="sb-mobile-palette-bar">
                    <div id="palette-icon" role="button" class="e-ddb-icons1 e-toggle-palette"></div>
                </div>
                <ejs-symbolpalette id="symbolpalette" height="900px" enableSearch="true" enableAnimation="true" getNodeDefaults="@getSymbolDefaults" getSymbolInfo="@getSymbolInfo" nodeTemplate="#nodeTemplate" palettes="@Model.palettes" symbolMargin="new SymbolPaletteMargin() { Left=15,Right=15,Top=15,Bottom=15}">
                </ejs-symbolpalette>
            </div>

            <div class="diagram-column">
                <ejs-diagram id="diagram" width="100%" height="900px" getNodeDefaults="@getNodeDefaults" nodeTemplate="#nodeTemplate">
                    <e-diagram-rulersettings showRulers="true"></e-diagram-rulersettings>
                    <e-diagram-pagesettings width="1500" Height="1500"></e-diagram-pagesettings>
                    <e-diagram-scrollsettings scrollLimit="Infinity"></e-diagram-scrollsettings>
                </ejs-diagram>
            </div>

            <div class="symbolpalette-property-section">
                <table id="property" title="Properties" style="width: 100%;height:100%;">
                    <tbody>
                        <tr style="height: 50px">
                            <td>
                                <div>
                                    Expandable
                                </div>
                            </td>
                            <td>
                                <div style="padding-left: 20px;">
                                    <ejs-dropdownlist id="expand" width="120px" dataSource="@Model.expand" value="expandValue" change="valueChanged"></ejs-dropdownlist>
                                </div>
                            </td>
                        </tr>
                        <tr style="height: 50px">
                            <td>
                                <div>
                                    Symbol Size
                                </div>
                            </td>
                            <td>
                                <div style="padding-left: 20px;">
                                    <ejs-numerictextbox id="size" format="##.##" value="50" max="90" min="40" step="5" change="sizeChanged" width="120px"></ejs-numerictextbox>
                                </div>
                            </td>
                        </tr>
                        <tr style="height: 50px">
                            <td>
                                <div style="padding-top: 14px;">
                                    Animation
                                </div>
                            </td>
                            <td>
                                <div style="padding-top: 9px;">
                                    <ejs-checkbox id="animation" checked="true" change="onAnimationChange"></ejs-checkbox>
                                </div>
                            </td>
                        </tr>
                        <tr style="height: 50px;">
                            <td>
                                <div style="padding-top: 14px;">
                                    Item Text
                                </div>
                            </td>
                            <td>
                                <div style="padding-top: 9px;">
                                    <ejs-checkbox id="itemtext" checked="false" change="onItemTextChange"></ejs-checkbox>
                                </div>
                            </td>
                        </tr>
                        <tr style="height: 50px;">
                            <td>
                                <div style="padding-top: 14px;">
                                    Header Icon
                                </div>
                            </td>
                            <td>
                                <div style="padding-top: 9px;">
                                    <ejs-checkbox id="headericon" checked="true" change="onHeaderIconChange"></ejs-checkbox>
                                </div>
                            </td>
                        </tr>
                        <tr style="height: 50px;">
                            <td>
                                <div style="padding-top: 14px;">
                                    Show Search TextBox
                                </div>
                            </td>
                            <td>
                                <div style="padding-top: 9px;">
                                    <ejs-checkbox id="showsearch" checked="true" change="onShowSearchBoxChange"></ejs-checkbox>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <style>
       /* Style to render icons on palettes*/
        @@font-face {
            font-family: 'e-ddb-icons';
            src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
            font-weight: normal;
            font-style: normal;
        }

        .e-ddb-icons {
            font-family: 'e-ddb-icons';
            speak: none;
            font-size: 16px;
            font-style: normal;
            font-weight: normal;
            font-variant: normal;
            text-transform: none;
            line-height: 1;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        .e-basic::before {
            content: "\e726";
        }

        .e-flow::before {
            content: "\e724";
        }

        .e-connector::before {
            content: "\e725";
        }

        .symbolpalette-property-section {
            overflow: auto;
            min-width: 22%
        }

        .symbolpalette-property-section table tr td div {
            padding-top: 18px;
            padding-bottom: 15px;
        }

        .diagram-symbolpalette {
            display: flex;
            flex-wrap: wrap;
            width: 100%;
        }

        .diagram-column {
            min-height: 600px;
            box-sizing: border-box;
        }

        .diagram-column {
            padding-right: 10px;
        }

        #symbolpalette, #diagram {
            width: 100%;
            height: 100%;
            border: 1px solid #ccc;
        }

        .palette-column {
            flex: 1 1 170px;
            min-width: 170px;
        }

        .diagram-column {
            flex: 3 1 500px;
            min-width: 400px;
        }

        .property-panel-section {
            flex: 1 1 200px;
            min-width: 180px;
        }

        .property-panel-header {
            padding-left: 10px;
            padding-top: 0px;
        }
    </style>
    <script type="text/javascript">
        var isItemText = false;
        var symbolSize = 50;
        var htmlSymbolWidth = 91;
        var htmlSymbolHeight = 100;

        function getNodeDefaults(symbol) {
            if (symbol.id.includes("BugFix")) {
                symbol.constraints = ej.diagrams.NodeConstraints.Default;
                symbol.tooltip.content = "";
            }
        }

        function getSymbolDefaults(symbol) {
            if (symbol.shape.type === 'HTML') {
                symbol.width = htmlSymbolWidth;
                symbol.height = htmlSymbolHeight;
            }
            symbol.style = { strokeWidth: 2, strokeColor: '#757575' };
        }

        function valueChanged(args) {
            var palette = document.getElementById("symbolpalette").ej2_instances[0];
            palette.expandMode = expand.value;
            palette.dataBind();
        }
        function getSymbolInfo(symbol) {
            if (symbol.shape.type === 'HTML') {
                return { width: htmlSymbolWidth, height: htmlSymbolHeight, fit: true };
            }
            return { width: symbolSize, height: symbolSize, fit: true };
        }

        function onAnimationChange(args) {
            var palette = document.getElementById("symbolpalette").ej2_instances[0];
            palette.enableAnimation = args.checked;
        }
        function onShowSearchBoxChange(args) {
            var palette = document.getElementById("symbolpalette").ej2_instances[0];
            palette.enableSearch = args.checked;
        }

        function onItemTextChange(args) {
            isItemText = args.checked;
            updateGetSymbolInfo();
        }
        function updateGetSymbolInfo() {
            var palette = document.getElementById("symbolpalette").ej2_instances[0];
            palette.getSymbolInfo = function (symbol) {
                return {
                    width: (symbol.shape.type === 'HTML') ? htmlSymbolWidth : symbolSize,
                    height: (symbol.shape.type === 'HTML') ? isItemText ? htmlSymbolHeight + 20 : htmlSymbolHeight : symbolSize,
                    fit: true,
                    description: { text: isItemText ? (symbol.id == "BugFix" ? "Bug Fix" : symbol.id) : '' }
                };
            };
            palette.dataBind();
        }

        function sizeChanged(args) {
            if (symbolSize != args.value) {
                symbolSize = args.value;
                updateGetSymbolInfo();
            }
        }

        function onHeaderIconChange(args) {
            var palette = document.getElementById("symbolpalette").ej2_instances[0];
            for (var i = 0; i < palette.palettes.length; i++) {
                if (args.checked) {
                    if (i === 0) {
                        palette.palettes[i].iconCss = 'e-ddb-icons e-basic';
                    } else if (i === 1) {
                        palette.palettes[i].iconCss = 'e-ddb-icons e-flow';
                    } else if (i === 2) {
                        palette.palettes[i].iconCss = 'e-ddb-icons e-connector';
                    }

                } else {
                    palette.palettes[i].iconCss = '';
                }
            }
            palette.dataBind();
        }
    </script>
