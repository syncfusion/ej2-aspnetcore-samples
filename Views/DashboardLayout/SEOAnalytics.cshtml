                                                                    <e-layersettings-marker visible="true" template="<div class='markerTemplate' id='marker2' style='font - size: 12px; color: white'>Australia</div>" dataSource="data2"></e-layersettings-marker>
                                                                    <e-layersettings-marker visible="true" template="<div class='markerTemplate' id='marker3' style='font - size: 12px; color: white'>Africa</div>" dataSource="data3"></e-layersettings-marker>
                                                                    <e-layersettings-marker visible="true" template="<div class='markerTemplate' id='marker4' style='font - size: 12px; color: white'>Europe</div>" dataSource="data4"></e-layersettings-marker>
                                                                    <e-layersettings-marker visible="true" template="<div class='markerTemplate' id='marker5' style='font - size: 12px; color: white; width:50px;'>North America</div>" dataSource="data5"></e-layersettings-marker>
