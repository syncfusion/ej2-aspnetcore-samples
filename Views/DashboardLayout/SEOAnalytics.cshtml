                                                            <e-series dataSource="ViewBag.LineData1" fill="rgb(14, 64, 152, .6)" name="Feb" xName="x" opacity=0.5 width=2 yName="y" type="@Syncfusion.EJ2.Charts.ChartSeriesType.SplineArea">
