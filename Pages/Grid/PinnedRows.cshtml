@model EJ2CoreSampleBrowser.Pages.Grid.PinnedRowsModel


@{
    ViewData["Title"] = "PinnedRows";
}

@section ControlsSection{
    <div class="control-section">
        @{
            var isRowPinned = "isRowPinned";
        }
        <ejs-grid id="PinnedRows" dataSource="@Model.datasource" allowSorting="true" isRowPinned="isRowPinned" height="300" allowFiltering="true" contextMenuItems="@(new List<object>() { "PinRow", "UnpinRow" })" enableVirtualization="true" allowKeyboard="false">
            <e-grid-filterSettings type="Excel"></e-grid-filterSettings>
            <e-grid-pageSettings pageSize="20"></e-grid-pageSettings>
            <e-grid-columns>
                <e-grid-column field="TicketID" headerText="Ticket ID" isPrimaryKey="true" width="140" freeze="Left"></e-grid-column>
                <e-grid-column field="Title" headerText="Title" width="210"></e-grid-column>
                <e-grid-column field="Description" headerText="Description" width="250" allowFiltering="false" clipMode="EllipsisWithTooltip"></e-grid-column>
