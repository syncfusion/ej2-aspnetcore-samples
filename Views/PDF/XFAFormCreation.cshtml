
@using Syncfusion.EJ2

@{ViewBag.Title = "    Essential PDF : XFA Form Creation : Syncfusion";}

@{Html.BeginForm("XFAFormCreation", "Pdf", FormMethod.Post); }
@section ControlsSection{
    <div class="control-section">
        @{Html.BeginForm("XFAFormCreation", "Pdf", FormMethod.Post);
            {
                <div class="LineSpace">
                </div>
                <div class="tablediv" style="font-size:14px">
                    <div class="rowdiv">
                        <div class="celldiv">
                            <table width="100%">
                                <tr>
                                    <td>
                                        Click the button to view a PDF document generated by Essential PDF. Please note
                                        that Adobe Reader or its equivalent is required to view the resultant document.
                                    </td>
                                </tr>
                                <tr>
                                    <td align="left">
                                        <br />
                                        <input id="createpdf" class="e-btn" type="submit" name="submit" value="Generate PDF" style="width:150px; text-transform:capitalize" />
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
                <br />
                Html.EndForm();
            }}
    </div>
}
@section ActionDescription{
    <div id="action-description" style="margin-top: 10px">
        This sample demonstrates how to create registration form using XFA form fields. The Essential PDF supports to create dynamic and static XFA form fields.
    </div>
}
@section Meta{
    <meta name="description" content="Example [C#] to create registration form using XFA form fields in ASP.NET Core using Syncfusion .NET PDF library." />
}
@section Description{
    <div id="description" style="margin-top: 10px">
        More information about the XFA can be found in this documentation <a href="https://help.syncfusion.com/file-formats/pdf/working-with-xfa">section.</a>
    </div>
}


