@page
@model EJ2CoreSampleBrowser_NET8.Pages.Diagram.AnnotationModel
@using Syncfusion.EJ2;
@using Syncfusion.EJ2.DropDowns;
@{
    var getNodeDefaults = "getNodeDefaults";
    var getConnectorDefaults = "getConnectorDefaults";
} 
@section ControlsSection{
    <div class="col-lg-8 control-section">
        <div class="content-wrapper">
            <ejs-diagram id="container" width="100%" height="645px" nodes="@Model.nodes" connectors="@Model.Connectors" getNodeDefaults="@getNodeDefaults" getConnectorDefaults="@getConnectorDefaults" selectionChange="selectionChange">
                <e-diagram-snapsettings constraints="None"></e-diagram-snapsettings>
            </ejs-diagram>
        </div>
    </div>
    <div class="col-lg-4 property-section">
        <div class="property-panel-header">
            Properties
        </div>
        <div class="row property-panel-content" id="appearance">
            <div class="row row-header" style="padding-top: 10px">
                Alignment
            </div>
            <div class="row">
                <div class="row" style="padding-top: 8px">
                    <div class="image-pattern-style" id="left" style="background-image: url('@Url.Content("~/images//Diagram/annotation/Annotation_1.png")'); margin-right: 4px">
                    </div>
                    <div class="image-pattern-style" id="right" style="background-image: url('@Url.Content("~/images//Diagram/annotation/Annotation_2.png")'); margin: 0px 4px">
                    </div>
                    <div class="image-pattern-style" id="bottoml" style="background-image: url('@Url.Content("~/images//Diagram/annotation/Annotation_3.png")');margin-left: 4px">
                    </div>
                </div>
                <div class="row" style="padding-top: 8px">
                    <div class="image-pattern-style" id="bottomr" style="background-image: url('@Url.Content("~/images//Diagram/annotation/Annotation_4.png")'); margin-right: 4px">
                    </div>
                    <div class="image-pattern-style" id="center" style="background-image: url('@Url.Content("~/images//Diagram/annotation/Annotation_5.png")'); margin: 0px 4px">
                    </div>
                    <div class="image-pattern-style" id="bottomcenter_top" style="background-image: url('@Url.Content("~/images//Diagram/annotation/Annotation_6.png")');margin-left: 4px">
                    </div>
                </div>
            </div>
        </div>
        <div class="row property-panel-content" style="padding-top: 10px">
            <div class="row row-header" style="padding-top: 10px">
                Appearance
            </div>
            <div class="row" style="padding-top: 8px">
                <div class="col-xs-4 column-style">
                    <ejs-button id="bold" disabled="false" cssClass="boldwidth" iconCss="e-ddb-icons e-bold"></ejs-button>
                </div>
                <div class="col-xs-4 column-style">
                    <ejs-button id="italic" disabled="false" cssClass="italicwidth" iconCss="e-ddb-icons e-italic"></ejs-button>
                </div>
                <div class="col-xs-4 column-style">
                    <ejs-button id="underline" disabled="false" cssClass="underlinewidth" iconCss="e-ddb-icons e-underline"></ejs-button>
                </div>
            </div>
            <div class="row" style="padding-top: 8px">
                <div class="col-xs-4 column-style">
                    <ejs-colorpicker id="fontColor" value="#000" change="fontColorChange"></ejs-colorpicker>
                </div>
                <div class="col-xs-4 column-style">
                    <ejs-numerictextbox id="fontSize" value="12" min="1" max="50" enabled="true" format="##.##" step="1" width="100%" change="textBoxChanged"></ejs-numerictextbox>
                </div>
                <div class="col-xs-4 column-style">
                    <ejs-dropdownlist id="fontFamily" enabled="true" created="Create" placeholder="Select a font type" index="0" popupWidth="150" dataSource="@Model.font" change="fontChange">
                        <e-dropdownlist-fields text="Name" value="Name"></e-dropdownlist-fields>
                    </ejs-dropdownlist>
                </div>
            </div>
        </div>
        <div class="row property-panel-content" style="padding-top: 10px">
            <div class="row row-header">
                Templates
            </div>
            <div class="row col-xs-8" style="padding-left: 0px; padding-top: 8px">
                <ejs-dropdownlist id="template" dataSource="@Model.templateList" popupWidth="200" width="100%" placeholder="select a template" change="onLabelTemplateChange">
                    <e-dropdownlist-fields text="Text" value="Value"></e-dropdownlist-fields>
                </ejs-dropdownlist>
            </div>
        </div>
        <div class="row" style="padding-top: 10px">
            <div class="row row-header">
                Behaviour
            </div>
            <div class="row" style="padding-top: 8px">
                <ejs-checkbox id="labelConstraints" label="Label interaction" checked="false" change="onLabelInteractionChange"></ejs-checkbox>
            </div>
        </div>
    </div>

    <style>
        #container {
            display: block;
        }
        /* Css for images in property panel  */
        .image-pattern-style {
            background-color: white;
            background-size: contain;
            background-repeat: no-repeat;
            height: 50px;
            width: calc((100% - 18px) / 3);
            cursor: pointer;
            border: 1px solid #D5D5D5;
            background-position: center;
            float: left;
        }
        /* To aligh the div of bold,italic and underline properly*/
        .boldwidth {
            width: 100%
        }

        .italicwidth {
            width: 100%
        }

        .underlinewidth {
            width: 100%
        }

        /* Selection indicator */
        .e-selected-style {
            border-color: #006CE6;
            border-width: 2px;
        }
        .property-section .e-remove-selection {
            cursor: not-allowed;
        }
        /* To align propery panel items */
        .column-style {
            display: table;
            height: 35px;
            padding-right: 4px;
            padding-left: 0px;
            width: calc((100% - 12px) / 3);
        }

        .row {
            margin-left: 0px;
            margin-right: 0px;
        }

        .row-header {
            font-size: 15px;
            font-weight: 500;
        }

        .e-checkbox-wrapper .e-label {
            font-size: 12px;
        }
        
        .col-xs-6 {
            padding-left: 0px;
            padding-right: 0px;
            padding-top: 5px;
        }
        /* To disable pointer events when there is no selection in diagram */
        .e-remove-selection .property-section-content {
            pointer-events: none;
        }

        /* These styles are used for property panel icons*/
        @@font-face {
            font-family: 'e-ddb-icons';
            src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
            font-weight: normal;
            font-style: normal;
        }

        .e-ddb-icons {
            font-family: 'e-ddb-icons';
            speak: none;
            font-size: 55px;
            font-style: normal;
            font-weight: normal;
            font-variant: normal;
            text-transform: none;
            line-height: 1;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        .e-bold::before {
            content: "\e712";
        }

        .e-italic::before {
            content: "\e717";
        }

        .e-underline::before {
            content: "\e716";
        }


        .row {
            display: block;
        }
    </style>
    <script>
        function selectionChange(arg) {
            if (arg.state === 'Changed') {
                var selectedElement = document.getElementsByClassName('e-selected-style');
                if (selectedElement.length) {
                    selectedElement[0].classList.remove('e-selected-style');
                }
                if (arg.newValue[0]) {
                    var node = arg.newValue[0];
                    var offset = node.annotations[0].offset;
                    if (offset.x === 0 && offset.y === 0) {
                        updatePosition('left');
                    } else if (offset.x === 1 && offset.y === 0) {
                        updatePosition('right');
                    } else if (offset.x === 1 && offset.y === 0) {
                        updatePosition('right');
                    } else if (offset.x === 0 && offset.y === 1) {
                        updatePosition('bottoml');
                    } else if (offset.x === 1 && offset.y === 1) {
                        updatePosition('bottomr');
                    } else if (offset.x === 0.5 && offset.y === 0.5) {
                        updatePosition('center');
                    } else if (offset.x === 0.5 && offset.y === 1) {
                        updatePosition('bottomcenter_top');
                    }

                }
                enableOptions(arg);
            }
        }
        function getNodeDefaults(node) {
            var obj = {
                width: 130, height: 50, style: { fill: '#D5EDED', strokeColor: '#7DCFC9', strokeWidth: 1 },
                shape: { cornerRadius: 5 }
            };
            return obj;
        }

        function getConnectorDefaults(obj) {
            obj.type = 'Orthogonal';
            obj.constraints = ej.diagrams.ConnectorConstraints.None;
            if (obj.id === 'connector5') {
                obj.segments = [{ direction: 'Right', length: 60, type: 'Orthogonal' }];
            }
            if (obj.id === 'connector6') {
                obj.segments = [{ direction: 'Bottom', length: 100, type: 'Orthogonal' }];
            }
            if (obj.id === 'connector7') {
                obj.segments = [{ direction: 'Left', length: 100, type: 'Orthogonal' }];
            }
            if (obj.id === 'connector8') {
                obj.segments = [{ direction: 'Top', length: 100, type: 'Orthogonal' }];
            }
        }
        var node;
        var connector;
        var bold = document.getElementById("bold");
        bold.onclick = function () {
            updateAnnotation('bold');
        };

        var italic = document.getElementById("italic");
        italic.onclick = function () {
            updateAnnotation('italic');
        };

        var underLine = document.getElementById("underline");
        underLine.onclick = function () {
            updateAnnotation('underline');
        };


        function textBoxChanged(args) {
            updateAnnotation('fontsize', args.value);
        }

        function Create() {
            var diagram = document.getElementById("container").ej2_instances[0];
            diagram.select([diagram.nodes[0]]);
        }

        function fontColorChange(args) {
            updateAnnotation('fontcolor', args.currentValue.rgba);
        }

        function fontChange(args) {
            updateAnnotation('fontfamily', args.value);
        }

        function onLabelTemplateChange(args) {
            updateAnnotation('template', args.value);
        }

        function onLabelInteractionChange(args) {
            updateAnnotation('interaction', '');
        }


        function updateAnnotation(name, value) {
            var diagram = document.getElementById("container").ej2_instances[0];
            for (var i = 0; i < diagram.selectedItems.nodes.length; i++) {
                var node = diagram.selectedItems.nodes[i];
                for (var j = 0; j < node.annotations.length; j++) {
                    var annotationStyle = node.annotations[j].style;
                    if (name === 'fontsize') {
                        annotationStyle.fontSize = value;
                    } else if (name === 'underline') {
                        annotationStyle.textDecoration = annotationStyle.textDecoration === 'Underline' ? 'None' : 'Underline';;
                    } else if (name === 'fontfamily') {
                        annotationStyle.fontFamily = value;
                    } else if (name === 'bold') {
                        annotationStyle.bold = true;
                    } else if (name === 'italic') {
                        annotationStyle.italic = true;
                    } else if (name === 'fontcolor') {
                        annotationStyle.color = value;
                    } else if (name === 'template') {
                        if (value === 'none') {
                            node.annotations[j].template = '';
                            node.annotations[j].width = undefined;
                            node.annotations[j].height = undefined;
                        } else {
                            node.annotations[j].width = 25;
                            node.annotations[j].height = 25;
                            node.annotations[j].template =
                                '<img src="../images/Diagram/annotation/' + value + '.svg" style="width:100%;height:100%" />';
                        }
                    } else if (name === 'interaction') {
                        node.annotations[j].constraints = node.annotations[j].constraints ^ ej.diagrams.AnnotationConstraints.Interaction;
                    }
                    diagram.dataBind();
                }
            }
        }
        
        document.getElementById('appearance').onclick = function (args) {
            var target = args.target;
            @*custom code start*@
            var selectedElement = document.getElementsByClassName('e-selected-style');
            if (selectedElement.length) {
                selectedElement[0].classList.remove('e-selected-style');
            }
            if (target.className === 'image-pattern-style') {
                updatePosition(target.id);
            }
        };
        @*custom code end*@
        function updatePosition(id) {
            var target = document.getElementById(id);
            var diagram = document.getElementById("container").ej2_instances[0];
            for (var i = 0; i < diagram.selectedItems.nodes.length; i++) {
                node = diagram.selectedItems.nodes[i];
                //we can refactor this code using a method
                for (var j = 0; j < node.annotations.length; j++) {
                    var annotation = node.annotations[j];
                    switch (target.id) {
                        case 'left':
                            annotation.offset.x = 0;
                            annotation.offset.y = 0;
                            annotation.verticalAlignment = 'Top';
                            annotation.horizontalAlignment = 'Left';
                            annotation.margin = { left: 3, top: 3 };
                            break;
                        case 'right':
                            annotation.offset.x = 1;
                            annotation.offset.y = 0;
                            annotation.verticalAlignment = 'Top';
                            annotation.horizontalAlignment = 'Right';
                            annotation.margin = { right: 3, top: 3 };
                            break;
                        case 'bottoml':
                            annotation.offset.x = 0;
                            annotation.offset.y = 1;
                            annotation.verticalAlignment = 'Bottom';
                            annotation.horizontalAlignment = 'Left';
                            annotation.margin = { left: 3, bottom: 3 };
                            break;
                        case 'bottomr':
                            annotation.offset.x = 1;
                            annotation.offset.y = 1;
                            annotation.verticalAlignment = 'Bottom';
                            annotation.horizontalAlignment = 'Right';
                            annotation.margin = { right: 3, bottom: 3 };
                            break;
                        case 'center':
                            annotation.offset.x = 0.5;
                            annotation.offset.y = 0.5;
                            annotation.verticalAlignment = 'Center';
                            annotation.horizontalAlignment = 'Center';
                            break;
                        case 'bottomcenter_top':
                            annotation.offset.x = .5;
                            annotation.offset.y = 1;
                            annotation.verticalAlignment = 'Top';
                            annotation.horizontalAlignment = 'Center';
                            break;
                    }
                    @*custom code start*@
                    target.classList.add('e-selected-style');
                    @*custom code end*@
                }
            }
        }
        @*custom code start*@
        function enableOptions(arg) {
            var appearance = document.getElementById('propertypanel');
            var selectedElement = document.getElementsByClassName('e-remove-selection');
            if (arg.newValue) {
                if (arg.newValue[0] instanceof ej.diagrams.Node) {
                    if (selectedElement.length) {
                        selectedElement[0].classList.remove('e-remove-selection');
                    }
                } else {
                    if (appearance && !appearance.classList.contains('e-remove-selection')) {
                        appearance.classList.add('e-remove-selection');
                    }
                }
            }
        }
        @*custom code end*@
    </script>

}
@section ActionDescription{
    <p>
        This sample illustrates the competitive environment of a business through five forces chart. The elements of the five force chart is described using nodes and annotations. Customizing the position and appearance of the annotation is illustrated in this example.
    </p>
}
@section Meta{
                    <meta name="description" content="This example demonstrates the Annotations in ASP.NET Core Diagram control. Explore here for more details."/>
                }

@section Description{
    <p>
        The <code>offset</code>, <code>horizontalAlignment</code>, and<code>verticalAlignment</code> properties of the annotation can be used to customize the position of the descriptions. The <code>bold</code>,<code>italic</code>, <code>fontSize</code>, and <code>fontFamily</code> properties can be used to customize the appearance of the descriptions.
    </p>
}
@section Title{
                    <title>ASP.NET Core Diagram Annotations Example - Syncfusion Demos </title> 
                }
@section Header{
                    <h1 class='sb-sample-text'>Example of Annotations in ASP.NET Core Diagram Control</h1>
                }