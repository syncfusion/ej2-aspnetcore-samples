@page
@model EJ2CoreSampleBrowser.Pages.SpeedDial.TemplateModel
@using Syncfusion.EJ2
@using Syncfusion.EJ2.Buttons

@section ControlsSection{
<div class="control-section">
    <div id="speed-dial-wrapper">
        <div class="speeddial-template-target fab-index" id="target">
            <ejs-speeddial id="speeddial"cssClass="popupSpeedDial" created="onCreated" target="#target" popupTemplate="#popupTemplate" content="Feedback" position="BottomLeft" openIconCss="speeddial-icons speeddial-icon-feedback"></ejs-speeddial>
            <ejs-speeddial id="speeddial1" target="#target" items="@Model.Items" itemTemplate="#itemTemplate" position="BottomRight" content="Edit" openIconCss="speeddial-icons speeddial-icon-edit" iconPosition="Right"></ejs-speeddial>
        </div>
    </div>
</div>
}
<script type="text/x-jsrender" id="popupTemplate">
    <div class="speeddial-form">
        <div class="head">
            <div class="textEle">Feedback & Question</div>
            <div class="iconEle"><span id="closebtn" class="speeddial-icons speeddial-icon-close closeicon"></span></div>
        </div>
        <div class="content">
            <input id="name" style="width:100%;"/>
            <br>
            <input id="email" style="width:100%;"/>
            <br>
            <textarea id="comment" style="width:100%;"></textarea>
            <br>
        </div>
        <div class="footer">
            <button id="primarybtn" class="e-btn e-success">Submit</button>
        </div>
    </div>
</script>
<script type="text/x-jsrender" id="itemTemplate">
        <div class="itemlist">
            <span class="icon ${iconCss}" style="padding:3px"></span>
            <span class="text" style="padding:0 5px">${text}</span>
        </div>
</script>
<script>
       function onCreated(){
             var textareaName = new ej.inputs.TextBox({
                placeholder: 'Enter your name',
                floatLabelType: 'Always',
                showClearButton:"true"
            });
            textareaName.appendTo('#name');
            var textareaEmail = new ej.inputs.TextBox({
               placeholder: 'Enter your e-mail',
               floatLabelType: 'Always',
               showClearButton:"true"
               });
            textareaEmail.appendTo('#email');
            var textareaComment = new ej.inputs.TextBox({
               placeholder: 'Share your comments',
               floatLabelType: 'Always'
               });
            textareaComment.appendTo('#comment');
            var speeddial = document.getElementById("speeddial").ej2_instances[0];
            document.getElementById('closebtn').onclick = function () {
                speeddial.hide();
            }
            document.getElementById('primarybtn').onclick = function () {
                speeddial.hide();
            }
        }
</script>

<style>
    .speeddial-template-target {
        min-height: 310px;
        width: 60%;
        margin: 0 auto 35px;
        position: relative;
        max-width: 600px;
    }

    .fab-index {
        z-index: 500 !important;
    }

    @@media only screen and (max-width: 600px) {
        .speeddial-template-target {
            width: 90%;
            margin: 0 5% 35px;
        }
    }
    /**Popup Template CSS  Start*/
    /**To Immediately hide the label elements in the popup template.*/
    .popupSpeedDial.e-speeddial-hidden {
        display: none;
    }

    .speeddial-form {
        width: 250px;
        border-radius: 5px;
        box-shadow: rgb(0 0 0 / 10%) 0px 10px 15px -3px, rgb(0 0 0 / 5%) 0px 4px 6px -2px;
        background: #f5f5f5;
    }

    .speeddial-form .content {
        padding: 10px 15px 0;
    }

    .speeddial-form .head {
        padding: 10px;
        background: transparent;
        position: relative;
        display: flex;
        align-items: end;
    }

    .speeddial-form .head .textEle {
        font-weight: bold;
        width: calc(100% - 25px);
        display: inline-block;
        font-size: 16px;
    }

    .speeddial-form .head .iconEle {
        width: 25px;
    }

    .speeddial-form .closeicon {
        float: right;
        cursor: pointer;
    }

    .speeddial-form .footer {
        padding: 0 15px 15px;
    }

    body[class*="dark"] .speeddial-form,
    body[class*="high"] .speeddial-form {
        background: #333;
    }

    .speeddial-form .content .e-float-input {
        margin-bottom: 15px;
    }

    .speeddial-form .footer .e-btn {
        margin: 0 60px;
        width: 100px;
    }

    .e-speeddial-li .itemlist {
        display: inherit;
        width: 100%;
        border: 1px solid transparent;
        align-items: center;
        padding: 5px;
        border-radius: 500px;
        background-color: rgba(104, 99, 104, 0.1);
        box-shadow: 0 0 4px grey;
    }

    .e-bigger .e-speeddial-li .itemlist {
        padding: 10px;
    }

    .e-speeddial-li .itemlist:hover,
    .e-speeddial-li.e-speeddial-li-active:not(.e-disabled) .itemlist {
        background-color: rgb(224, 224, 224);
    }

    body[class*="dark"] .e-speeddial-li .itemlist:hover,
    body[class*="dark"] .e-speeddial-li.e-speeddial-li-active:not(.e-disabled) .itemlist,
    body[class*="high"] .e-speeddial-li .itemlist:hover,
    body[class*="high"] .e-speeddial-li.e-speeddial-li-active:not(.e-disabled) .itemlist {
        background-color: rgb(117,117,117);
    }

    @@font-face {
        font-family: 'speeddial-icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    [class^="speeddial-icon-"], [class*= "speeddial-icon-"] {
        font-family: 'speeddial-icons' !important;
        speak: none;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: inherit;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .speeddial-icon-cut:before {
        content: "\e70a";
    }

    .speeddial-icon-copy:before {
        content: "\e70b";
    }

    .speeddial-icon-paste:before {
        content: "\e70c";
    }

    .speeddial-icon-delete:before {
        content: "\e70d";
    }

    .speeddial-icon-edit:before {
        content: "\e70f";
    }

    .speeddial-icon-feedback:before {
        content: "\e710";
    }
    .speeddial-icon-close:before {
        content: "\e700";
    }
</style>


@section Meta{
<meta name="description" content="This example demonstrates the ASP.NET Core SpeedDial Linear Direction control. Explore here for more details." />
    }

@section ActionDescription{
<div id="action-description">
    <p>
        The following sample demonstrates customization of action items and speed dial popup using templates. Click the Speed Dial button to open action items.
    </p>
</div>
}
@section Description{
<div id="description">
    <p>
        In the above example, action items are customized using the <code>itemTemplate</code> and popup is customized to load custom UI using the <code>popupTemplate</code>.
    </p>
</div>
}

@section Title{
<title>ASP.NET Core Speed Dial Template Example - Syncfusion Demos </title>
              }
@section Header{
<h1 class='sb-sample-text'>Example of Template in ASP.NET Core SpeedDial Control</h1>
               }